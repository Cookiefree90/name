# Purpose: Example environment variables for the AVG Financial Expert Demo.
# Based on avg.json content_prompt for env_example_file.
# Copy this file to .env and fill in your specific values.

# Google ADK and Vertex AI Configuration
GOOGLE_GENAI_USE_VERTEXAI=True
GOOGLE_CLOUD_PROJECT="YOUR_GCP_PROJECT_ID_HERE"
GOOGLE_CLOUD_LOCATION="YOUR_GCP_REGION_HERE" # e.g., us-central1

# ADK Logging Level
ADK_LOG_LEVEL="INFO" # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL

